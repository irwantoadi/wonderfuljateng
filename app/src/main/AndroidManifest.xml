<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.irwantostudio.wonderfuljateng">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowKabupatenActivity"
            android:parentActivityName=".KabupatenActivity"/>
        <activity android:name=".ShowKulinerActivity"
            android:parentActivityName=".KulinerActivity"/>
        <activity android:name=".ShowWisataActivity"
            android:parentActivityName=".WisataActivity"/>
        <activity android:name=".KabupatenActivity"
            android:label="Daftar Kabupaten"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ProfilActivity"
            android:label="Profil Saya"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".KulinerActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".BudayaActivity"
            android:label="Budaya"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".HotelActivity"
            android:label="Penginapan"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".UmkmActivity"
            android:label="UMKM"
            android:parentActivityName=".MainActivity"/>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-8539740865663154~4032155957" />

        <activity
            android:name=".WisataActivity"
            android:label="Wisata"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="Wonderful Jateng"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>